{% extends 'base.html.twig' %}

{% block body %}
<div>
    {% if users|length > 0 %}
        <ul class="list-group">
            {% for user in users %}
                <li class="list-group-item">{{ user.name }}</li>
            {% endfor %}
        </ul>
    {% else %}
        <h2 style="margin: 0;"><small>No managers found</small></h2>
    {% endif %}
</div>
{% endblock %}